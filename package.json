{
    "name": "nodejs-resume-api",
    "main": "src/server.ts",
    "private": true,
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev-node": "ts-node -r tsconfig-paths/register src/server.ts",
        "dev": "nodemon -r tsconfig-paths/register --exec ts-node ./src/server.ts",
        "babel-dev": "nodemon --exec babel-node src/server.js",
        "nodemon": "nodemon --exec babel-node ./src/server.js",
        "babel-start": "npm run remove-build && npm run build-babel && node ./build/server.js",
        "babel-build": "babel ./src -d ./build",
        "remove-build": "rm -rf build",
        "start": "NODE_ENV=production node dist/server.js",
        "nodemon-start": "nodemon dist/server.js",
        "build-tsc": "tsc && npm run copy",
        "build": "tsc && npm run copy",
        "copy": "cp -R ./src/views ./src/public ./dist/",
        "test": "jest --passWithNoTests"
    },
    "author": "DatVT",
    "license": "ISC",
